# В СЕКУНДАХ
delta_t=0.000001

# В МЕТРАХ
delta_x=0.0001
delta_y=0.0001

# Кількість циклів обрахунку. Час в симуляції = cycles * delta_t
cycles=1000000

# Один раз в стільки циклів зображення буде збережене для анімації
cycles_per_image=10000

# Якщо поле alpha є в конфігурації, то поля ro, k, c не будуть враховані.
# alpha = k / (ro * c)
#alpha=1000

# !!!УВАГА. Будьте уважними до одиниць вимірювання. У цьому файлі величини, що відповідають алюмінію

# Коефіціент теплопровідності.
#  Вт / (м * K)  |  Watt / (meter * Kelvin)
k=237

# Питома теплоємність
# Дж / (кг * К)  |  Джоуль / (кілограм * Кельвін)
c=920

# Густина
#  кг / м^3  |  Kilogram / (meter^3)
ro=2698.9
